package is207.connectn.handin.glenn;

import java.util.HashSet;
import java.util.concurrent.Callable;



public class FindLearnedMove implements Callable<Integer[]>
{
    HashSet<String> list;
    private String moves;

    public FindLearnedMove(HashSet<String> list, String moves) {
        this.list = list;
        this.moves = moves;
    }

    public Integer[] call() {
        for (String s : list) {
            if (s.startsWith(moves)) {
                return new Integer[] {(int) Integer.parseInt(Character.toString(s.charAt(moves.length()))), 100};
            }
        }
        return new Integer[] {0, 0};
    }

}
