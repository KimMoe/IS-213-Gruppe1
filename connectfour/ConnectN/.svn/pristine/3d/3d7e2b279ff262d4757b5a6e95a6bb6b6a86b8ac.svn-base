package is207.connectn.game.strategy;

/**
 *
 * @author evenal
 */
public class TimeOutStrategy extends Strategy
{

    int fib(int n) {
        switch (n) {
        case 1:
            return 1;
        case 2:
            return 1;
        default:
            return fib(n - 1) + fib(n - 2);
        }
    }

    @Override
    public Integer call() {
        for (int i = 0; i < validMoves.size(); i++) {
            selectedMove = validMoves.get(i);

            //simulate heavy computation
            int dummy = fib(40);
        }
        return selectedMove;
    }

}
