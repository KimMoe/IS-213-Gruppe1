package is207.connectn.game.strategy;

import is207.connectn.game.Strategy;
import java.util.Random;



/**
 *
 * @author evenal
 */
public class RandomStrategy extends Strategy {

    private int[] stackHeight = new int[cols];
    Random rand = new Random();


    @Override
    public Integer call() {
        int move = rand.nextInt(validMoves.size());
        move = validMoves.get(move);
        return move;
    }
}
