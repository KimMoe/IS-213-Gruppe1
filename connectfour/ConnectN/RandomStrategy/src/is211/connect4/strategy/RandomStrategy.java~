import java.util.Random;



/**
 *
 * @author evenal
 */
public class RandomStrategy extends AbstractStrategy
{

    private Random random = new Random();

    public Integer call() {
        int i = random.nextInt(validMoves.size());
        return validMoves.get(i);
    }
}
